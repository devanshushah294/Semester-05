@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model System.Data.DataTable

<!DOCTYPE html>
<html>
<head>
    <title>Country View</title>
</head>
<body>
    <h1>Country View</h1>

    <table>
        <thead>
            <tr>
                <th>Country ID</th>
                <th>Country Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (System.Data.DataRow row in Model.Rows)
            {
                <tr>
                    <td>@row["CountryID"]</td>
                    <td>@row["CountryName"]</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
